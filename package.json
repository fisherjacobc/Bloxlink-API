{
  "name": "@codiium/bloxlink-api",
  "version": "4.1.1",
  "description": "An unofficial Bloxlink API wrapper for Node.js",
  "typings": "./index.d.ts",
  "exports": {
    "./global": {
      "types": "./global.d.ts",
      "import": "./global.mjs",
      "require": "./global.js"
    },
    "./guild": {
      "types": "./guild.d.ts",
      "import": "./guild.mjs",
      "require": "./guild.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "publish": "tsc && npm publish --access public",
    "example:globalsearch": "node ./examples/globalsearch.js",
    "example:guildsearch": "node ./examples/guildsearch.js"
  },
  "keywords": [
    "bloxlink",
    "bloxlink-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "discord-api-types": "^0.37.45"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "typescript": "^5.1.3"
  }
}
